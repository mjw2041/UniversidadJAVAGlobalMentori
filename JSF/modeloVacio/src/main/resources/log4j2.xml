<?xml version="1.0" encoding="UTF-8"?>

<configuration status="INFO"> 
    <Appenders> 
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d(HH:mm:SS)[%t] %-5level %looger[36] -%msg%n"> </PatternLayout>            
        </console>
    </Appenders>
    <Loggers>
        <Root Level="info">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>

</configuration>